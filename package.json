{
  "name": "soos",
  "description": "Docker Managed NPM",
  "config": {
    "dockerScope": "ngparty",
    "ghooks": {
      "pre-commit": "npm test"
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "main": "index.js",
  "bin": {
    "soos": "bin/soos"
  },
  "scripts": {
    "test": "eslint . && ava **/*.spec.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ngParty/soos.git"
  },
  "keywords": [
    "npm",
    "docker"
  ],
  "author": "@elmariofredo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ngParty/soos/issues"
  },
  "homepage": "https://github.com/ngParty/soos#readme",
  "devDependencies": {
    "ava": "^0.7.0",
    "commitizen": "^2.5.0",
    "cz-conventional-changelog": "^1.1.5",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "semantic-release": "^4.3.5",
    "semantic-release-cli": "^1.4.1"
  },
  "dependencies": {
    "shasum": "^1.0.2",
    "yargs": "^3.31.0"
  },
  "engines": {
    "node": ">=4.1.2"
  }
}